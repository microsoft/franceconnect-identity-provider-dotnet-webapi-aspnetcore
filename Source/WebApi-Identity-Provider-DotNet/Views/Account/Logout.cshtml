@using WebApi_Identity_Provider_DotNet.Models.Account
@model LogoutViewModel

<div class="logout-page">
    <div class="lead">
        <h1>Deconnexion</h1>
        <p>Souhaitez vous vous deconnecter ?</p>
    </div>

    <form asp-action="Logout">
        <input type="hidden" name="logoutId" value="@Model.LogoutId"  />
        <div class="form-group">
            <button class="btn btn-primary">Oui</button>
        </div>
    </form>
</div>
